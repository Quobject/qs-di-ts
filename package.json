{
  "name": "qs-di-ts",
  "version": "1.0.1",
  "description": "Lightweight wrap and extension of di.js v2 for TypeScript.",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "build": "rimraf dist/ && tsc",
    "test-spec": "ts-node node_modules/blue-tape/bin/blue-tape.js \"src/**/*.spec.ts\" | tap-diff",
    "test": "npm run lint && npm run build && npm run test-spec",
    "prepublish": "npm run lint && npm run build && rimraf dist/**/*.spec.* "
  },
  "keywords": [
    "dependency injection",
    "di",
    "typescript",
    "di.js"
  ],
  "author": "Matthias Ludwig",
  "license": "ISC",
  "dependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.46",
    "@types/reflect-metadata": "0.0.5",
    "@types/typescript": "^0.4.29",
    "@types/blue-tape": "^0.1.30",
    "di": "^2.0.0-pre-14",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "expectations": "^0.4.2",
    "grunt": "^0.4.5",
    "grunt-bump": "^0.3.0",
    "mocha": "^2.2.4",
    "rimraf": "^2.5.2",
    "tap-diff": "^0.1.1",
    "tslint": "^3.10.2",
    "ts-node": "^0.7.3",
    "typescript": "^2.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Quobject/qs-di-ts"
  }
}
